{
  "name": "HTML5 FileSystem Explorer",
  "version": "0.72",
  "description": "Show HTML5 stored files through File System API.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "file://*/*"
  ],
  "manifest_version": 2
}
